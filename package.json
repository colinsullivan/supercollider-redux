{
  "name": "supercollider-redux",
  "version": "0.0.97",
  "description": "State synchronization between a replica SuperCollider state store and a primary Redux state store.",
  "main": "lib/index.js",
  "browser": "lib/index_browser.js",
  "scripts": {
    "test": "mocha --timeout 10000 --slow 4000 --require @babel/register",
    "build": "rm -Rf lib && babel src --out-dir lib",
    "build_docs": "plantuml -tpng docs/*.puml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colinsullivan/supercollider-redux.git"
  },
  "keywords": [
    "node.js",
    "supercollider",
    "sclang",
    "redux"
  ],
  "author": "Colin Sullivan <colin [at] colin-sullivan.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinsullivan/supercollider-redux/issues"
  },
  "homepage": "https://github.com/colinsullivan/supercollider-redux#readme",
  "dependencies": {
    "osc": "^2.3.1",
    "redux": "^3.7.2",
    "supercolliderjs": "^0.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "mocha": "^6.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error"
    }
  }
}
