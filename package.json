{
  "name": "supercollider-redux",
  "version": "0.0.97",
  "description": "State synchronization between a replica SuperCollider state store and a primary Redux state store.",
  "main": "lib/index.js",
  "browser": "lib/index_browser.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --require @babel/register --require source-map-support/register --timeout 10000 --slow 4000 --bail",
    "build": "rm -Rf lib && babel src --out-dir lib",
    "build_docs": "plantuml -tpng docs/*.puml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colinsullivan/supercollider-redux.git"
  },
  "keywords": [
    "node.js",
    "supercollider",
    "sclang",
    "redux"
  ],
  "author": "Colin Sullivan <colin [at] colin-sullivan.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinsullivan/supercollider-redux/issues"
  },
  "homepage": "https://github.com/colinsullivan/supercollider-redux#readme",
  "dependencies": {
    "@supercollider/lang": "^1.0.0-alpha.1",
    "@supercollider/scapi": "^1.0.0-alpha.0",
    "osc": "^2.3.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error"
    }
  }
}
